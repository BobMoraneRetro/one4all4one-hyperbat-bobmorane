				<animation property="opacity" from="0" to="1" begin="650" duration="500" mode="Linear"/>
				<animation property="offsetX" from="1" to="0" begin="650" duration="500" mode="Linear"/>
			</storyboard>
		</image>
		<image name="box_def" extra="true">
			<path>{game:boxart}</path>
			<visible>${IfnoextGamethemeexist}</visible>
			<origin>0.5 0.5</origin>
			<pos>0.817708333333 0.700462962963</pos>
			<maxSize>0.167708333333 0.519444444444</maxSize>
			<zIndex>260</zIndex>
			<scaleOrigin>-0.3 -0.3</scaleOrigin>
			<rotationOrigin>0.5 0.5</rotationOrigin>
			<linearSmooth>true</linearSmooth>
			<storyboard event="open">
				<animation property="opacity" from="0" to="0" begin="" duration="" mode="Linear"/>
				<animation property="opacity" from="0" to="1" begin="250" duration="500" mode="Linear"/>
				<animation property="offsetX" from="7.9" to="0" duration="750" mode="EaseOutQuint"/>
			</storyboard>
			<storyboard event="activateNext">
				<animation property="opacity" from="0" to="0" begin="" duration="" mode="Linear"/>
				<animation property="opacity" from="0" to="1" begin="250" duration="500" mode="Linear"/>
				<animation property="offsetX" from="7.9" to="0" duration="750" mode="EaseOutQuint"/>
			</storyboard>
			<storyboard event="activatePrev">
				<animation property="opacity" from="0" to="0" begin="" duration="" mode="Linear"/>
				<animation property="opacity" from="0" to="1" begin="250" duration="500" mode="Linear"/>
				<animation property="offsetX" from="7.9" to="0" duration="750" mode="EaseOutQuint"/>
			</storyboard>
		</image>
		<video name="PreviewVid_def" extra="true">
			<path>{game:video}</path>
			<visible>${IfnoextGamethemeexist}</visible>
