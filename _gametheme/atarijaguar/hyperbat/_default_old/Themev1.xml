			<maxSize>0.259646069046 0.363601199110</maxSize>
			<origin>0.5 0.5</origin>
			<rotationOrigin>0.5 0.5</rotationOrigin>
			<delay>0.4</delay>
			<effect>none</effect>
			<zIndex>270</zIndex>
		</video>
		<image name="Videobezel_def" extra="true">
			<path>./video.png</path>
			<visible>${IfnoextGamethemeexist}</visible>
			<origin>0.5 0.5</origin>
			<pos>0.473958333333 0.429358862779</pos>
			<size>0.305208333333 0.430648228734</size>
			<zIndex>280</zIndex>
			<scaleOrigin>-0.3 -0.3</scaleOrigin>
			<rotationOrigin>0.5 0.5</rotationOrigin>
			<linearSmooth>true</linearSmooth>
		</image>
		<image name="md_cartridge" extra="true">
			<visible>false</visible>
			<zIndex>290</zIndex>
		</image>
		<image name="md_boxart" extra="true">
			<visible>false</visible>
			<zIndex>290</zIndex>
		</image>
		<control name="Artwork_def, PreviewVid_def, Videobezel_def, box_def, console1_def, console2_def, logo_def, marquee_def">
			<storyboard event="deactivateNext">
				<animation property="scale" to="3" begin="" autoreverse="false" duration="200" mode="easyIn"/>
				<animation property="opacity" from="1" to="0" begin="100" duration="150" mode="linear"/>
			</storyboard>
			<storyboard event="deactivatePrev">
				<animation property="scale" to="3" begin="" autoreverse="false" duration="200" mode="easyIn"/>
				<animation property="opacity" from="1" to="0" begin="100" duration="150" mode="linear"/>
			</storyboard>
		</control>
	</customView>
</theme>
