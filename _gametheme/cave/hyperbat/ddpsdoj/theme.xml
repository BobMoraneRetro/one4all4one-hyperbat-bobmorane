				<animation property="shader.innerShadowSize"  to="0.01" begin="1200" duration="100" mode="Linear"/>
				<animation property="offsetY" from="1" to="0" begin="800" duration="500" mode="Linear"/>
			</storyboard>
		</control>
		<!-- ################ Animation de sortie des artworks ##################### -->
		<control name="Artwork1, Artwork21, Artwork22, Artwork23, Artwork31, Artwork32, Artwork33, PreviewVid,Videobezel">
			<storyboard event="deactivateNext">
				<animation property="scale" to="3" begin="" autoreverse="false" duration="200" mode="easyIn"/>
				<animation property="opacity" from="0" to="0" begin="100" duration="150" mode="linear"/>
			</storyboard>
			<storyboard event="deactivatePrev">
				<animation property="scale" to="3" begin="" autoreverse="false" duration="200" mode="easyIn"/>
				<animation property="opacity" from="0" to="0" begin="100" duration="150" mode="linear"/>
			</storyboard>
		</control>
		<image name="md_cartridge" extra="true">
			<visible>false</visible>
		</image>
		<image name="md_boxart">
			<visible>false</visible>
		</image>
	</customView>
</theme>