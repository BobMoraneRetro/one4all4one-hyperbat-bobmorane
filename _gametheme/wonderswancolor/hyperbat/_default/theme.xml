				<animation property="scaleOrigin" to="0 0" begin="" duration="" mode="Linear"/>
				<animation property="opacity" from="0" to="1" begin="1500" duration="800" mode="Linear"/>
				<animation property="scale" from="0" to="1.1" begin="1500" duration="500" mode="EaseIn" autoreverse="false"/>
				<animation property="scale" from="1.1" to="0.95" begin="2000" duration="300" mode="bump" autoreverse="false"/>
