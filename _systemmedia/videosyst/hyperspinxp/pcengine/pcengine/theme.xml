			<pos>0.38 0.210648148148148</pos>
			<size>0.327604166666667 0.321296296296296</size>
			<linearSmooth>true</linearSmooth>
			<!-- # Arrivee : G->D # -->
			<storyboard repeatAt="1000">
				<animation property="opacity" from="0" to="0" begin="" duration="" mode="Linear"/>
				<animation property="opacity" from="0" to="1" begin="200" duration="500" mode="Linear"/>
				<animation property="offsetY" from="-1" to="0" begin="0" duration="1000" mode="linear"/>
				<animation property="offsetX" from="0" to="0.2" begin="1350" duration="120" autoreverse="true" mode="EasyOutCubic"/>
				<animation property="offsetX" from="0" to="0.10" begin="1590" duration="120" autoreverse="true" mode="EasyOutCubic"/>
				<animation property="offsetX" from="0" to="0.05" begin="1830" duration="100" autoreverse="true" mode="EasyOutCubic"/>
				<animation property="offsetX" from="0" to="0.02" begin="2030" duration="100" autoreverse="true" mode="EasyOutCubic"/>
				<animation property="opacity" from="1" to="1" begin="2200" duration="5800" mode="Linear"/>
			</storyboard>
		</image>
