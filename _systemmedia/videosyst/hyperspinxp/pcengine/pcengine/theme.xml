			<!-- # Arrivee D->G Sweet # -->
			<storyboard>
				<animation property="opacity" from="0" to="0" begin="" duration="" mode="Linear"/>
				<animation property="opacity" from="0" to="1" begin="1300" duration="600" mode="Linear"/>
