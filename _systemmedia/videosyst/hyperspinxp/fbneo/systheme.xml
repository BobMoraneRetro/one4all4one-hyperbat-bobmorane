			<pos>0.49375 0.54212962962963</pos>
			<size>0.403125 0.54537037037037</size>
			<zIndex>3</zIndex>
			<storyboard>
				<animation property="opacity" from="0" to="0" begin="" duration="" mode="Linear"/>
				<animation property="opacity" from="0" to="1" begin="1200" duration="800" mode="Linear"/>
				<!-- <animation property="w" from="0" to="0.505208333333333" begin="1200" duration="800" mode="EaseOut"/> -->
			</storyboard>
		</image>
		<!-- ################ La video ##################### -->
		<video name="PreviewVid" extra="true">
			<visible>true</visible>
			<path>{random}</path>
			<maxSize>0.45 0.70</maxSize>
			<origin>0.5 0.5</origin>
			<delay>0.3</delay>
			<showSnapshotNoVideo>false</showSnapshotNoVideo>
			<showSnapshotDelay>false</showSnapshotDelay>
			<effect>none</effect>
			<pos>0.50 0.52</pos>
			<zIndex>2</zIndex>
			<!-- <shader> -->
