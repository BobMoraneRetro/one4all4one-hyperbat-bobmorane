		<!-- ################ Artwork1 ##################### -->
		<image name="Artwork1" extra="true">
			<path>./artwork1.png</path>
			<scaleOrigin>-0.3 -0.3</scaleOrigin>
			<origin>0.5 0.5</origin>
			<zIndex>2</zIndex>
			<linearSmooth>true</linearSmooth>
			<pos>0.512239583333333 0.112962962962963</pos>
			<size>0.574479166666667 0.161111111111111</size>
			<!-- # Arrivee D->G Sweet # -->
			<storyboard>
				<animation property="opacity" from="0" to="0" begin="" duration="" mode="Linear"/>
				<animation property="opacity" from="0" to="1" begin="00" duration="400" mode="Linear"/>
				<animation property="offsetY" from="-1" to="0" begin="00" duration="800" mode="EaseOut"/>
			</storyboard>
		</image>
		<!-- ################ Artwork2 ##################### -->
		<image name="Artwork2" extra="true">
			<path>./Artwork2.png</path>
			<scaleOrigin>-0.3 -0.3</scaleOrigin>
			<origin>0.5 0.5</origin>
			<zIndex>40</zIndex>
			<pos>0.7875 0.551388888888889</pos>
			<size>0.425 0.897222222222222</size>
			<linearSmooth>true</linearSmooth>
			<!-- # Arrivee : G->D # -->
			<storyboard>
				<animation property="opacity" from="0" to="0" begin="" duration="" mode="Linear"/>
				<animation property="opacity" from="0" to="1" begin="1250" duration="500" mode="Linear"/>
				<animation property="scale" from="0" to="1" begin="1250" duration="500" mode="bump"/>
				<animation property="scale" from="1" to="1.1" begin="1750" duration="120" autoreverse="true" mode="EasyOutCubic"/>
				<animation property="scale" from="1" to="1.05" begin="1870" duration="120" autoreverse="true" mode="EasyOutCubic"/>
			</storyboard>
		</image>
		<!-- ################ Artwork3 ##################### -->
		<image name="Artwork3" extra="true">
			<path>./Artwork3.png</path>
			<scaleOrigin>-0.3 -0.3</scaleOrigin>
			<origin>0.5 0.5</origin>
			<zIndex>4</zIndex>
			<pos>0.197135416666667 0.541666666666667</pos>
			<size>0.228645833333333 0.916666666666667</size>
		<!-- # Arrivee Centrale Zoom puis dezoom en position # -->
			<storyboard>
				<animation property="opacity" from="0" to="0" begin="" duration="" mode="Linear"/>
				<animation property="opacity" from="0" to="1" begin="750" duration="500" mode="Linear"/>
				<animation property="offsetX" from="-1" to="0" begin="750" duration="500" mode="bump"/>
				<animation property="offsetX" from="0" to="0.1" begin="1250" duration="120" autoreverse="true" mode="EasyOutCubic"/>
				<animation property="offsetX" from="0" to="0.05" begin="1490" duration="120" autoreverse="true" mode="EasyOutCubic"/>
				<animation property="offsetX" from="0" to="0.02" begin="1730" duration="100" autoreverse="true" mode="EasyOutCubic"/>
			</storyboard>
		</image>
