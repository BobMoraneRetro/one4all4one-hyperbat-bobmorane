				<animation property="offsetY" from="0" to="-0.005" begin="3000" duration="200" autoreverse="true" mode="easeOut"/>
			</storyboard>
		</image>
		<!-- ################ Bezel autour de la video ##################### -->
		<image name="Videobezel" extra="true">
			<path>./video.png</path>
			<origin>0.5 0.5</origin>
			<pos>0.5 0.563888888888889</pos>
			<size>0.883333333333333 0.824074074074074</size>
			<zIndex>2</zIndex>
			<storyboard>
