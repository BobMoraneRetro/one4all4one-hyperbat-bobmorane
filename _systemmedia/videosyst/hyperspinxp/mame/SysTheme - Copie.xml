				<animation property="offsetY" from="0" to="-0.005" begin="3000" duration="200" autoreverse="true" mode="easeOut"/>
			</storyboard>
		</image>
		<!-- ################ La video ##################### -->
		<video name="PreviewVid" extra="true">
			<visible>true</visible>
			<!-- <path>./4.mp3</path> -->
			<path>{random}</path>
			<maxSize>0.51 0.54</maxSize>
			<minSize>0.5 0.54</minSize>
			<origin>0.5 0.5</origin>
			<delay>0.3</delay>
			<showSnapshotNoVideo>false</showSnapshotNoVideo>
			<showSnapshotDelay>false</showSnapshotDelay>
			<effect>none</effect>
			<pos>0.38 0.565</pos>
			<zIndex>3</zIndex>
			<shader>
				<path>:/shaders/border.glsl</path>
				<cornerRadius>0.03</cornerRadius>
				<borderSize>0.005</borderSize>
				<borderColor>000000</borderColor>
				<innerShadowSize>0.01</innerShadowSize>
				<innerShadowColor>00000030</innerShadowColor>
				<outerShadowSize>0.020</outerShadowSize>
				<outerShadowColor>00000050</outerShadowColor>
			</shader>
		</video>
