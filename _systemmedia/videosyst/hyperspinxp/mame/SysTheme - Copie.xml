			<zIndex>1</zIndex>
			<pos>0.378385416666667 0.564351851851852</pos>
			<size>0.552604166666667 0.562037037037037</size>
			<!-- # Arrivee D->G Sweet # -->
			<storyboard>
				<animation property="opacity" from="0" to="0" begin="" duration="" mode="Linear"/>
				<animation property="opacity" from="0" to="1" begin="1700" duration="800" mode="Linear"/>
				<animation property="scale" from="10" to="1" begin="1700" duration="1000" mode="easeOutCubic"/>
				<animation property="scaleOrigin" to="0 0" begin="1700" duration="" mode="Linear"/>
				<animation property="opacity" from="1" to="0" begin="3200" duration="500" mode="Linear"/>
				<animation property="offsetX" from="0" to="0.01" begin="2600" duration="200" autoreverse="true" mode="easeOut"/>
				<animation property="offsetY" from="0" to="-0.01" begin="2600" duration="200" autoreverse="true" mode="easeOut"/>
